{
  "expo": {
    "name": "klaviyo plugin example",
    "slug": "klaviyo-plugin-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "bundleIdentifier": "com.klaviyo.expoexample",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.klaviyo.expoexample",
      "jsEngine": "hermes",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#ffffff",
        "defaultChannelId": "default"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "klaviyo-expo-plugin",
        {
          "android": {
            "logLevel": 2,
            "openTracking": false
          },
          "ios": {}
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "android_package_name": "com.klaviyo.expoexample",
          "ios_bundle_id": "com.klaviyo.expoexample"
        }
      ],
      [
        "@react-native-firebase/messaging",
        {
          "android_package_name": "com.klaviyo.expoexample",
          "ios_bundle_id": "com.klaviyo.expoexample"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "projectId": "test-push-project-kl"
  }
}
